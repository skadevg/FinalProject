<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.cibertec.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash_image"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".ConsultaClienteActivity"></activity>
        <activity android:name=".ListaClientesActivity"></activity>
        <activity android:name=".MantenimientoClienteActivity"></activity>
        <activity android:name=".MenuPrincipalActivity"></activity>
        <activity android:name=".NuevoClienteActivity"></activity>
    </application>

</manifest>